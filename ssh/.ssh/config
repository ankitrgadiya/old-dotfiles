# Git
Host github.com
  HostName ssh.github.com
  User git
  Port 443
  IdentityFile ~/.ssh/git

Host gitlab.com
  HostName altssh.gitlab.com
  User git
  Port 443
  IdentityFile ~/.ssh/git

Host aur.archlinux.org
  HostName aur.archlinux.org
  User aur
  Port 22
  IdentityFile ~/.ssh/git

Host repo.or.cz
  HostName repo.or.cz
  User git
  Port 22
  IdentityFile ~/.ssh/git

Host arlo.git
  HostName arlo
  User git
  Port 9096
  IdentityFile ~/.ssh/git

Host euclid.git
  HostName euclid.git
  User git
  Port 22
  IdentityFile ~/.ssh/git

# Server
Host arlo
  HostName arlo
  User elliot
  Port 9096
  IdentityFile ~/.ssh/server

# Remote
Host tty.argp.in
  HostName tty.argp.in
  User pi
  Port 9001
  IdentityFile ~/.ssh/server

# Local
Host rpi.local
  HostName rpi.local
  User pi
  Port 22
  IdentityFile ~/.ssh/server

Host euclid.local
  HostName euclid.local
  User elliot
  Port 22
  IdentityFile ~/.ssh/server

Host debian.local
  HostName debian.local
  User elliot
  Port 22
  IdentityFile ~/.ssh/server

Host arch.local
  HostName arch.local
  User elliot
  Port 22
  IdentityFile ~/.ssh/server

